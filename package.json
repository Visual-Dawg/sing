    "@types/jsdom": "21.1.1",
    "@types/node": "20.1.0",
    "@types/source-map-support": "0.5.6",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@vitest/ui": "0.31.0",
    "ansicolor": "1.1.100",
    "clsx": "1.2.1",
    "concurrently": "8.0.1",
    "cross-env": "7.0.3",
    "dequal": "2.0.3",
    "electron": "24.2.0",
    "electron-builder": "23.6.0",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.2.0",
    "electron-log": "4.4.8",
    "electron-store": "8.1.0",
