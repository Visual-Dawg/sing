{
  "name": "sing",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "test:renderer-watch": "vitest  -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,svelte",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "svelte-check --tsconfig packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.26",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.41",
    "@testing-library/svelte": "^3.1.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jsdom": "^16.2.14",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@vitest/ui": "^0.9.4",
    "autoprefixer": "^10.4.4",
    "cross-env": "7.0.3",
    "electron": "17.1.0",
    "electron-builder": "22.14.13",
    "electron-devtools-installer": "3.2.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "jsdom": "^19.0.0",
    "playwright": "1.19.1",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "simple-git-hooks": "2.7.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.5",
    "tailwindcss": "^3.0.24",
    "typescript": "4.5.5",
    "unplugin-icons": "^0.14.1",
    "vite": "^2.9.1",
    "vitest": "0.5.4"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "ansicolor": "^1.1.100",
    "electron-store": "^8.0.1",
    "electron-updater": "4.6.5",
    "music-metadata": "^7.12.3",
    "prisma": "^3.12.0",
    "slash": "^3.0.0",
    "svelte-devtools-standalone": "github:RedHatter/svelte-devtools-standalone"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
