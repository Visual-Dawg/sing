    "electron-updater": "5.3.0",
    "esbuild": "0.16.12",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.27.5-1",
    "eslint-plugin-svelte": "2.27.3",
    "eslint-plugin-unicorn": "47.0.0",
    "fishery": "2.2.2",
    "fp-ts": "2.14.0",
    "fp-ts-std": "0.16.0",
    "fs-extra": "11.1.1",
