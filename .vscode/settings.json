{
  "search.exclude": {
    "**/testdata/**/*": true,
    "**/.vscode/*": true,
    "./.vscode/*": true,
    "**/package-lock.json": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.autoImportFileExcludePatterns": [
    "node_modules/fp-ts/lib/Bifunctor",
    "node_modules/fp-ts/lib/Bounded",
    "node_modules/fp-ts/lib/Console",
    "node_modules/fp-ts/lib/Date",
    "node_modules/fp-ts/lib/EitherT",
    "node_modules/fp-ts/lib/FromThese",
    "node_modules/fp-ts/lib/FunctorWithIndex",
    "node_modules/fp-ts/lib/IO",
    "node_modules/fp-ts/lib/IOEither",
    "node_modules/fp-ts/lib/IOOption",
    "node_modules/fp-ts/lib/JSON",
    "node_modules/fp-ts/lib/Map",
    "node_modules/fp-ts/lib/Monoid",
    "node_modules/fp-ts/lib/Number",
    "node_modules/fp-ts/lib/Option",
    "node_modules/fp-ts/lib/OptionT",
    "node_modules/fp-ts/lib/Task",
    "node_modules/fp-ts/lib/TaskEither",
    "node_modules/fp-ts/lib/These",
    "node_modules/fp-ts/lib/TheseT",
    "node_modules/fp-ts/"
  ],
  "explorer.fileNesting.patterns": {
    "*.svelte": "${basename}Helper.ts"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "svelte.plugin.svelte.compilerWarnings": {
    "a11y-accesskey": "ignore",
    "a11y-aria-attributes": "ignore",
    "a11y-autofocus": "ignore",
    "a11y-click-events-have-key-events": "ignore",
    "a11y-distracting-elements": "ignore",
    "a11y-hidden": "ignore",
    "a11y-img-redundant-alt": "ignore",
    "a11y-incorrect-aria-attribute-type": "ignore",
    "a11y-invalid-attribute": "ignore",
    "a11y-label-has-associated-control": "ignore",
    "a11y-media-has-caption": "ignore",
    "a11y-misplaced-role": "ignore",
    "a11y-misplaced-scope": "ignore",
    "a11y-missing-attribute": "ignore",
    "a11y-missing-content": "ignore",
    "a11y-mouse-events-have-key-events": "ignore",
    "a11y-no-abstract-role": "ignore",
    "a11y-no-redundant-roles": "ignore",
    "a11y-positive-tabindex": "ignore",
    "a11y-role-has-required-aria-props": "ignore",
    "a11y-structure": "ignore",
    "a11y-unknown-aria-attribute": "ignore",
    "a11y-unknown-role": "ignore"
  }
}
